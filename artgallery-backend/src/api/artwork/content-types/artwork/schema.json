{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "singularName": "artwork",
    "pluralName": "artworks",
    "displayName": "Artwork"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Artist": {
      "type": "string"
    },
    "Price": {
      "type": "decimal"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    }
  }
}
